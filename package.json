{
  "name": "get-trello-board",
  "version": "1.0.0",
  "description": "Retrieve a Trello board's information by ID or URL.",
  "author": "Luke Horvat",
  "license": "MIT",
  "repository": "lukehorvat/get-trello-board",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "node_modules/.bin/babel lib -d dist -q",
    "prebuild": "node_modules/.bin/rimraf dist",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "got": "6.6.3",
    "is-url": "1.2.2",
    "yargs": "4.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-node6": "11.0.0",
    "rimraf": "2.5.4"
  },
  "keywords": [
    "trello",
    "board",
    "url",
    "id",
    "api"
  ],
  "babel": {
    "presets": [
      "node6"
    ]
  }
}
